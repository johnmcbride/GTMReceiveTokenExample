<Window x:Class="GTMReceiveTokenExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel LastChildFill="True">
            <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
                <TextBlock>GTM API Key:</TextBlock>
                <TextBox Name="gtmAPIKey"></TextBox>
            </StackPanel>
            <Button DockPanel.Dock="Top" Click="Button_Click">Get Token</Button>
            <WebBrowser Name="web1" Navigating="web1_Navigating"></WebBrowser>
        </DockPanel>
    </Grid>
</Window>
